@article{ai-cataract-complications,
	title        = {Application of Artificial Intelligence in the Analysis of Features Affecting Cataract Surgery Complications in a Teaching Hospital},
	author       = {Lanza, Michele and Koprowski, Robert and Boccia, Rosa and Krysik, Katarzyna and Sbordone, Sandro and Tartaglione, Antonio and Ruggiero, Adriano and Simonelli, Francesca},
